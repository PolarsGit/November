local Level130 = false
CreateToggle("Level 130+", "Quest Farms", function()
    Level130 = not Level130
    
    local Npc = GetRemotes(Vector3.new(-5181.75, -449.374, -3817.56))
    
    local Remote1, Remote2 = Npc:FindFirstChild("QuestDone"), Npc:FindFirstChild("Done")
    
    local Target = "Sewer Vampire"
    if Level130 == true then
        while Level130 == true and task.wait() do
            pcall(function()
                for i, v in pairs(game:GetService("Workspace").Living:GetChildren()) do
                    if v.Name == Target and v:FindFirstChild("Humanoid").Health ~= 0 and Level130 == true then
                        Remote1:FireServer()
                        Remote2:FireServer()
                        
                        repeat
                            task.wait()
                            pcall(function()
                                for i, v in pairs(game:GetService("Players").LocalPlayer.PlayerGui.CDgui.fortnite:GetChildren()) do
                                    if v:IsA("Frame") and v.Textt.Text == "Punch" then
                                        --// waiting
                                    else
                                        game:GetService("Players").LocalPlayer.Character.StandEvents.M1:FireServer()
                                    end
                                end
                                if game:GetService("Players").LocalPlayer.Character:FindFirstChild("Stand") then
                                    game:GetService("Players").LocalPlayer.Character.Stand:WaitForChild("HumanoidRootPart").CFrame = game:GetService("Players").LocalPlayer.Character:WaitForChild("HumanoidRootPart").CFrame
                                end
                                if UndergroundMode == true then
                                    game:GetService("Players").LocalPlayer.Character:WaitForChild("HumanoidRootPart").CFrame = v:FindFirstChild("HumanoidRootPart").CFrame * CFrame.new(0, -6, 4, 1, 0, 0, 1) -- -593.952698, 66.6276474, -783.119934, -0.707110405, -5.96460623e-08, -0.707103133, -8.71555841e-08, 1, 2.80376944e-09, 0.707103133, 6.36105568e-08, -0.707110405
                                    if game:GetService("Players").LocalPlayer.Character:FindFirstChild("Stand") then
                                        game:GetService("Players").LocalPlayer.Character.Stand:WaitForChild("HumanoidRootPart").CFrame = game:GetService("Players").LocalPlayer.Character:WaitForChild("HumanoidRootPart").CFrame
                                    end
                                else
                                    if game:GetService("Players").LocalPlayer.Character:FindFirstChild("Stand") then
                                        game:GetService("Players").LocalPlayer.Character.Stand:WaitForChild("HumanoidRootPart").CFrame = game:GetService("Players").LocalPlayer.Character:WaitForChild("HumanoidRootPart").CFrame
                                    end
                                    game:GetService("Players").LocalPlayer.Character:WaitForChild("HumanoidRootPart").CFrame = v:FindFirstChild("HumanoidRootPart").CFrame * CFrame.new(0, 0, 4)
                                end
                                game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.Velocity = Vector3.new(0, 0, 0)
                                if game:GetService("Players").LocalPlayer.Character:FindFirstChild("Aura").Value == false   then
                                    game:GetService("Players").LocalPlayer.Character.StandEvents.Summon:FireServer()
                                end
                            end)
                        until v:FindFirstChild("Humanoid").Health == 0 or Level130 == false
                        
                        if AutoRespawn == true and Level130 == true then
                            game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.Position + Vector3.new(0, 0, 1000))
                        end
                        if UndergroundMode == true and Level130 == true then
                            game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.Position + Vector3.new(0, 10, 0))
                        end
                    end
                end
            end)
        end
    end
end)

local Level300 = false
CreateToggle("Level 300+", "Quest Farms", function()
    Level300 = not Level300
    
    local Npc = GetRemotes(Vector3.new(-668.287, 66.1782, -184.107))
    
    local Remote1, Remote2 = Npc:FindFirstChild("QuestDone"), Npc:FindFirstChild("Done")
    
    local Target = "Pillerman"
    if Level300 == true then
        while Level300 == true and task.wait() do
            pcall(function()
            for i, v in pairs(game:GetService("Workspace").Living:GetChildren()) do
                if v.Name == Target and v:FindFirstChild("Humanoid").Health ~= 0 and Level300 == true then
                    Remote1:FireServer()
                    Remote2:FireServer()
                    
                    repeat
                        task.wait()
                        pcall(function()
                            for i, v in pairs(game:GetService("Players").LocalPlayer.PlayerGui.CDgui.fortnite:GetChildren()) do
                                if v:IsA("Frame") and v.Textt.Text == "Punch" then
                                    --// waiting
                                else
                                    game:GetService("Players").LocalPlayer.Character.StandEvents.M1:FireServer()
                                end
                            end
                            if game:GetService("Players").LocalPlayer.Character:FindFirstChild("Stand") then
                                game:GetService("Players").LocalPlayer.Character.Stand:WaitForChild("HumanoidRootPart").CFrame = game:GetService("Players").LocalPlayer.Character:WaitForChild("HumanoidRootPart").CFrame
                            end
                            if UndergroundMode == true then
                                game:GetService("Players").LocalPlayer.Character:WaitForChild("HumanoidRootPart").CFrame = v:FindFirstChild("HumanoidRootPart").CFrame * CFrame.new(0, -6, 4, 1, 0, 0, 1) -- -593.952698, 66.6276474, -783.119934, -0.707110405, -5.96460623e-08, -0.707103133, -8.71555841e-08, 1, 2.80376944e-09, 0.707103133, 6.36105568e-08, -0.707110405
                            else
                                game:GetService("Players").LocalPlayer.Character:WaitForChild("HumanoidRootPart").CFrame = v:FindFirstChild("HumanoidRootPart").CFrame * CFrame.new(0, 0, 4)
                            end
                            game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.Velocity = Vector3.new(0, 0, 0)
                            if game:GetService("Players").LocalPlayer.Character:FindFirstChild("Aura").Value == false   then
                                game:GetService("Players").LocalPlayer.Character.StandEvents.Summon:FireServer()
                            end
                        end)
                    until v:FindFirstChild("Humanoid").Health == 0 or Level300 == false
                    
                    if AutoRespawn == true and Level300 == true then
                        game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.Position + Vector3.new(0, 0, 1000))
                    end
                    if UndergroundMode == true and Level300 == true then
                        game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.Position + Vector3.new(0, 10, 0))
                    end
                end
            end
            end)
        end
    end
end)
