local Amount = 0
CreateTextBox("How Many To Purchase", "Auto Buy", {CustomText = "Amount"}, function(A)
    if tonumber(A) then
        if tonumber(A) >= 1 then
            Amount = tonumber(A)
            CreateNotification("Arctic", "Amount Set To : "..tostring(Amount), {})
        else
            CreateNotification("Arctic", "Please Set The Number Above 0+", {})
        end
    end
end)

