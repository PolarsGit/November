CreateMenu("Auto Pickup")

local function IsSafe(A)
    local IsSafe = false
    for i, v in pairs(game:GetService("Workspace").ItemSpawns:GetChildren()) do
        if IsSafe == false then
            if (A - v.Position).Magnitude < 15 then
                IsSafe = true
                return true
            end
        end
    end
end

CreateNote("// Settings //", "Auto Pickup")

local TeleportTimer = 0.5
CreateTextBox("Teleport Cooldown", "Auto Pickup", {CustomText = "Rec 0.5"}, function(Input)
    TeleportTimer = tonumber(Input)
end)

CreateBar("Auto Pickup", {})

local AutoPickup = false
CreateToggle("Auto Pickup", "Auto Pickup", function()
    AutoPickup = not AutoPickup
    
    if AutoPickup then
        while AutoPickup and task.wait() do
            pcall(function()
                for i, v in pairs(game.Workspace.Items:GetChildren()) do
                    task.wait(TeleportTimer)
                    
                    local SafeCheck = IsSafe(v.Handle.Position)
                    
                    if SafeCheck == true and v:IsA("Tool") then
                        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(v.Handle.Position)
                        game.Players.LocalPlayer.Character:WaitForChild(v.Name)
                        game.Players.LocalPlayer.Character.Humanoid:UnequipTools()
                    end
                end
            end)
        end
    end
end)

