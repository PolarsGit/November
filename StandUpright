CreateMenu("Drop Page")

local SetTool
local Cooldown = 0

CreateNote("// Settings //", "Drop Page")

CreateTextBox("Drop Cooldown", "Drop Page", {CustomText = "Rec. 0.1-0.3"}, function(Input)
    Cooldown = tonumber(Input)
    
    CreateNotification("Project Polar", "Cooldown set to : "..tostring(Cooldown))
end)

CreateButton("Select Tool", "Drop Page", function()
    if game.Players.LocalPlayer.Character:FindFirstChildOfClass("Tool") then
        SetTool = game.Players.LocalPlayer.Character:FindFirstChildOfClass("Tool").Name
        CreateNotification("Project Polar", "Tool set to : "..SetTool)
        CreateNotification("Project Polar", "If this does not work, equip a random item and do /dropitem and try again.")
    else
        CreateNotification("Project Polar", "Equip a tool to drop.")
    end
end)

CreateBar("Drop Page", {})

local AutoDrop = false
CreateToggle("Auto Drop", "Drop Page", function()
    AutoDrop = not AutoDrop
    
    if SetTool then
        while task.wait() and AutoDrop do
            if game.Players.LocalPlayer.Backpack[SetTool] then
                task.wait(Cooldown)
                
                game:GetService("Players").LocalPlayer.Character:FindFirstChildOfClass("Humanoid"):EquipTool(game:GetService("Players").LocalPlayer.Backpack[SetTool])
                game:GetService("Players").LocalPlayer.PlayerGui.Chat.Frame.ChatBarParentFrame.Frame.BoxFrame.Frame.ChatBar:CaptureFocus()
                game:GetService("Players").LocalPlayer.PlayerGui.Chat.Frame.ChatBarParentFrame.Frame.BoxFrame.Frame.ChatBar.Text = "/dropitem"
                keypress(0x0D)
            else
                CreateNotification("Project Polar", "There are no more "..SetTool.."'s in your inventory.")
            end
        end
    else
        CreateNotification("Project Polar", "Set a tool before starting.")
    end
end)

local HandItem = false
CreateTextBoxToggle("Auto Hand Item", "Drop Page", {CustomText = "Player Name"}, function(Input)
    HandItem = not HandItem
    
        if SetTool then
        while task.wait() and HandItem do
            if game.Players.LocalPlayer.Backpack[SetTool] then
                task.wait(Cooldown)
                
                game:GetService("Players").LocalPlayer.Character:FindFirstChildOfClass("Humanoid"):EquipTool(game:GetService("Players").LocalPlayer.Backpack[SetTool])
                game:GetService("Players").LocalPlayer.PlayerGui.Chat.Frame.ChatBarParentFrame.Frame.BoxFrame.Frame.ChatBar:CaptureFocus()
                game:GetService("Players").LocalPlayer.PlayerGui.Chat.Frame.ChatBarParentFrame.Frame.BoxFrame.Frame.ChatBar.Text = "/handitem "..Find(Input)
                keypress(0x0D)
            else
                CreateNotification("Project Polar", "There are no more "..SetTool.."'s in your inventory.")
            end
        end
    else
        CreateNotification("Project Polar", "Set a tool before starting.")
    end
end)

CreateMenu("Lair Page")

local Distance = 10

CreateNote("// Farm Settings //", "Lair Page")

CreateTextBox("Set Distance : ", "Lair Page", {CustomText = "Rec. 10"}, function(Input)
    Distance = Input
end)

CreateBar("Lair Page", {})

CreateNote("// Lairs //", "Lair Page")

local function GetRemote(Position)
    local Remote
    
    for i, v in pairs(FolderDirectory:GetDescendants()) do
        if v.Name == "HumanoidRootPart" then
            if (v.Position - Position).Magnitude < 15 then
                Remote = v.Parent.Done
            end
        end
    end
    
    if Remote then
        return Remote
    else
        CreateNotification("Project Polar", "Failed to get a remote.")
    end
end

local BadGi = false
CreateToggle("Bad Gi Leader", "Lair Page", function()
    BadGi = not BadGi
    
    if BadGi then
        while BadGi and task.wait() do
            pcall(function()
                local FireRemote = GetRemote(Vector3.new(-727.007, 67.0773, -939))
                
                if FireRemote then
                    FireRemote:FireServer()
                    
                    local Boss = game:GetService("Workspace").Living:WaitForChild("Boss")
                    
                    pcall(function()
                        repeat
                            task.wait()
                            
                            pcall(function()
                                if game:GetService("Players").LocalPlayer.Character:FindFirstChild("Stand").Head.Transparency == 1 then
                                    game:GetService("Players").LocalPlayer.Character.StandEvents.Summon:FireServer()
                                end
                                
                                game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = Boss:FindFirstChild("HumanoidRootPart").CFrame * CFrame.new(0, -Distance, 3, 1, 0, 0, 1)
                                game.Players.LocalPlayer.Character.StandEvents.M1:FireServer()
                                game.Players.LocalPlayer.Character.HumanoidRootPart.Velocity = Vector3.new(0, 0, 0)
                                game.Players.LocalPlayer.Character.Stand.HumanoidRootPart.CFrame = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame
                            end)
                        until Boss.Humanoid.Health == 0 or BadGi == false or game:GetService("Players").LocalPlayer.Character:FindFirstChild("Humanoid").Health == 0
                        
                        game.Players.LocalPlayer.CharacterAdded:Wait()
                        
                        repeat task.wait() until game.Players.LocalPlayer.Character:FindFirstChild("HumanoidRootPart")
                    end)
                else
                    CreateNotification("Project Polar", "Issue Obtaining Remote.")
                end
            end)
        end
    end
end)

