local AutoUmbrella = false
CreateToggle("Auto Umbrella", "Local Player", function()
    AutoUmbrella = not AutoUmbrella
    
    if AutoUmbrella then
        while AutoUmbrella and task.wait() do
            pcall(function()
                if game.Players.LocalPlayer.Backpack:FindFirstChild("Umbrella") and not game.Players.LocalPlayer.Character:FindFirstChild("Umbrella") then
                    game.Players.LocalPlayer.Character.Humanoid:EquipTool(game.Players.LocalPlayer.Backpack:FindFirstChild("Umbrella"))
                end
            end)
        end
    end
end)

local KetchupSell = false
CreateToggle("Auto Sell Ketchup", "Local Player", function()
    KetchupSell = not KetchupSell
    
    if KetchupSell then
        while KetchupSell and task.wait() do
            if game.Players.LocalPlayer.Backpack:FindFirstChild("Ketchup") then
                game:GetService("ReplicatedStorage").Events.SellKetchup:FireServer("Lots")
            end
        end
    end
end)

